import java.util.Scanner;
public class Good{
     public static void main(String[]args){
           Scanner sc=new Scanner(System.in);
            int num=sc.nextInt();
            int sum=0;
            int temp=num;
            while(num>0){
                 int digits=num%10;
                 sum+=digits;
                 num/=10;
                
             }
              if(temp%sum==0){
                       System.out.println("Good number");
                  }else{
                          System.out.println("Bad number");
}
}
}
