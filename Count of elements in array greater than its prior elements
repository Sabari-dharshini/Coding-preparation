import java.util.Scanner;
public class greater{
    public static void main(String[]args){
      Scanner sc= new Scanner(System.in);
       int n=sc.nextInt();
       int a[]=new int[n];
       for(int i=0;i<n;i++){
             a[i]=sc.nextInt();
             }
     int result=a[0];
     int count=1;
      for(int i=0;i<n;i++){
          if(a[i]>result){
             count++;
             
           }
         }
     System.out.println(count);
}
}
